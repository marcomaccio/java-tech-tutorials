<?xml version="1.0" encoding="UTF-8"?>
<schema
        xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:tns="http://marmac.name/tutorials/java/jaxb/xmlclientserver/session/client/"

        targetNamespace="http://marmac.name/tutorials/java/jaxb/xmlclientserver/session/client/"
        attributeFormDefault="unqualified"
        elementFormDefault="qualified" >


    <!-- XMLClientSessionType definition -->
    <complexType name="XMLClientSessionType">
        <sequence>
          <element type="tns:newAssetType" name="newAsset"/>
        </sequence>
    </complexType>


    <complexType name="newAssetType">
        <sequence>
          <element type="string" name="description"/>
        </sequence>
        <attribute type="string" name="url"/>
        <attribute type="string" name="type"/>
    </complexType>
</schema>